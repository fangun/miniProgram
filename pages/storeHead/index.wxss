/* pages/storeHead/index.wxss */

page {
  height: 100%;
}
.authorize{
	position: fixed;width: 100vw;height: 100vh;z-index: 10000;background: transparent
}
.authorize2{
	position: absolute;width: 690rpx;height: 80rpx;background: transparent
}
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

page>scroll-view {
  height: 100%;
}

.modal, .container .modal {
  width: 100%;
  height: 100%;
  position: fixed;
  z-index: 599;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.modal image {
  width: 630rpx;
  height: 915rpx;
}

.modal>view {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.modal_trynow {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.modal_trynow>view {
  width: 100%;
  height: 88rpx;
  line-height: 88rpx;
  font-size: 28rpx;
  text-align: center;
  
}

.container {
  padding: 0;
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
  font-family: "siyuanhei";
}

.container>view {
  margin-bottom: 10rpx;
  width: 100%;
}

.topTitle {
  width: 100%;
  height: 88rpx;
  font-size: 34rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  position: fixed;
  top: 0;
  z-index: 499;
}

.topTitle .btn {
  width: 37rpx;
  height: 37rpx;
}

/* .topTitle>view{width: 100%;text-align: center} */

.topTitle .btn:nth-child(1) {
  margin-left: 30rpx;
}

.topTitle .btn:nth-child(3) {
  margin-right: 30rpx;
}

.topTip {
  height: 100rpx;
  width: 100%;
  position: fixed;
  z-index: 399;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 28rpx;
  margin-top: -12rpx;
}

.topTip image {
  width: 28rpx;
  height: 28rpx;
  margin-right: 15rpx;
}

.show {
  top: 0rpx;
  margin-top: 80rpx;
  animation: show 1s;
  animation-timing-function: ease;
}

@keyframes show {
  from {
    margin-top: -12rpx;
  }

  to {
    margin-top: 88rpx;
  }
}

.hide {
  margin-top: -12rpx;
  animation: hide 2s;
  animation-timing-function: ease-in-out;
}

@keyframes hide {
  from {
    margin-top: 88rpx;
  }

  to {
    margin-top: -12rpx;
  }
}


.container>view:last-child, .container #tip {
  margin-bottom: 0;
}

/*商家信息*/

.info {
  width: 100%;
  margin-top: 80rpx;
}

#infoBlock {
  margin: 30rpx;
  padding-top: 26rpx;
  padding-bottom: 29rpx;
  border-radius: 5rpx;
}

.intro {
  margin: 0 26rpx 0 26rpx;
  display: flex;
}

#headImage {
  width: 138rpx;
  height: 138rpx;
  flex-shrink: 0;
  background-position: center;
  background-size: 100%;
  border-radius: 10rpx;
}

#nameintro {
  margin-left: 48rpx;
  flex-direction: column;
}

.storeTitle {
  font-size: 34rpx;
  color: rgb(51, 51, 51);
  display: flex;
  align-items: center;
}
.storeTitle text{
	font-weight:bold;
	padding-left:4px;height:18px;
	line-height:18px
}

.image_ding {
  width: 29rpx;
  height: 23rpx;
  margin-left: 8rpx;
}

.introText {
  margin-top: 14rpx;
}

.introText>text {
  display: block;
  font-size: 24rpx;
  line-height: 32rpx;
  height: 64rpx;
  color: rgb(102, 102, 102);
  overflow: hidden;
	width: 400rpx
}

.introText>.ellipsis {
  font-size: 24rpx;
  height: 32rpx;
  line-height: 32rpx;
}

.introText>.open {
  height: auto;
}

#infoBlock>text {
  display: block;
  text-align: right;
  margin-right: 21rpx;
  font-size: 24rpx;
  color: rgb(102, 102, 102);
  margin-top: -16rpx;
}

#contact {
  margin: 38rpx 0 0 26rpx;
}

#contact>view {
  display: flex;
  align-items: center;
  justify-content: flex-start;
}

#contact>view:nth-child(1) {
  margin-bottom: 18rpx;
}

.image_location {
  width: 20rpx;
  height: 27rpx;
  margin-right: 15rpx;
}

.image_tel {
  width: 27rpx;
  height: 27rpx;
  margin-right: 8rpx;
}

#contact text {
  font-size: 24rpx
}

.tel_right {
  display: flex;
  flex-grow: 1;
  align-items: center;
}

.tel_right text {
  display: block;
  /* flex-grow: 1; */
	min-width: 200rpx
}

.moreIntro {
  width: 80rpx;
  height: 32rpx;
  font-size: 14rpx;
  border: 1rpx solid;
  border-radius: 50rpx;
  line-height: 32rpx;
  text-align: center;
  margin-right: 24rpx;
}

.outBtn {
  display: flex;
}

.outBtn image {
  width: 32rpx;
  height: 32rpx;
  background-position: center;
  background-size: 100%;
  margin-right: 26rpx;
}

.outBtn image:last-child {
  margin-right: 24rpx;
}

/*选择服务*/

#selectService {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.block_title {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  font-size: 34rpx;
  margin: 24rpx 0;
  font-weight: bold;
}

.block_title image {
  height: 65rpx;
  /* width: 241rpx; */
	width: 493rpx
}

.block_title text {
  margin-top: -60rpx;
  height:65rpx;
  line-height: 65rpx;
}

#projects {
  display: flex;
  width: 666rpx;
  margin: 0 46rpx 40rpx 46rpx;
  flex-wrap: wrap;
  justify-content: flex-start;
}

.project {
