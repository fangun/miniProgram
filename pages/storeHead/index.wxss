.modal_tip>.modal_btns {
  margin-top: 54rpx;
  justify-content: space-between;
  width: 100%;
}

.modal_btns>view {
  width: 220rpx;
  height: 60rpx;
  line-height: 60rpx;
  border: 2rpx solid rgb(243, 67, 67);
  text-align: center;
  border-radius: 8rpx;
  margin-bottom: 40rpx;
}

.modal_btns>view:nth-child(1) {
