}

.modal_tip .modal_title {
  font-size: 32rpx;
  margin-top: 48rpx;
  margin-bottom: 36rpx;
  text-align: center;
}

.modal_content {
  display: flex;
  flex-direction: column;
  align-items: flex-start;
}

.modal_content>view {
  line-height: 180%;
}

.modal_tip .modal_btn {
  background: rgb(243, 67, 67);
	color: white;
