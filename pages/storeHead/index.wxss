  display: flex;
}

.sub {
  width: 140rpx;
}

.modal_tip>.tip {
  width: 100%;
  text-align: left;
  color: rgb(153, 153, 153);
}

.shortInfo .modalTip, .orderClash .modalTip, .lessOne .modalTip {
  background: white;
  width: 650rpx;
  display: flex;
  flex-direction: column;
  border-radius: 16rpx;
}

.modalTipText {
