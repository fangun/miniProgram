.sBlack .date{color:#8c8c8c}
.sBlack .dateSelected {background: #3d3d3d;color: white;border-radius: 200rpx;border: 4rpx solid #ffe33f;box-sizing: border-box}
.sBlack .time{border: 2rpx solid #ffe33f;color: #ffe33f}
.sBlack .fillTimeBlock{background:#333;color:#333;border:0}
.sBlack .timeUnable {background: #3d3d3d;border-color: #3d3d3d;color: #999;}
.sBlack .timeSelect {background: #ffe33f;border-color: #ffe33f;color: #333;}
.sBlack .form{border: 2rpx solid #ffe33f;}
.sBlack .form>view{color: white}
.sBlack .form>.formBorder {background: #ffe33f}
.sBlack #tip input {border: 2rpx solid #ffe33f;color: white}
.sBlack .btn_submit{color: #333;background: #ffe33f}
.sBlack .topTip{background: #ffe33f;color: #333}
.sBlack .modal>.modal_tip {background: #333;color: white}
.sBlack .modal_tip .modal_btn {background: #ffe33f;color: #ffe33f}
.sBlack .modal_btns>view {border: 2rpx solid #ffe33f;}
.sBlack .modal_btns>view:nth-child(1) {color: #ffe33f;}
.sBlack .modal_btns>view:nth-child(2) {color: #333;background: #ffe33f;}
.sBlack .btn_ui{background: #E9CC25}
.sBlack .modal_btns>view:nth-child(1).btn_ui{background:#222}
.sBlack .modal_btns>view:nth-child(2).btn_ui{background:#E9CC25}