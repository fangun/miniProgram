  display: inline-flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  border-radius: 10rpx;
  font-size: 26rpx;
  margin-left: 50rpx;
}

.date:nth-child(1) {
  margin-left: 20rpx;
}

.rightCalendar {
  width: 42rpx;
  height: 44rpx;
  margin-right: 33rpx;
  background-size: 100%;
}

.selectHour {
  display: flex;
  flex-wrap: wrap;
  text-align: center;
  width: 650rpx;
  justify-content: space-between;
  margin-top: 34rpx;
}

.time {
  width: 140rpx;
  height: 56rpx;
  line-height: 56rpx;
  border-radius: 8rpx;
