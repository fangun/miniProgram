  align-items: center;
}

.selectInfo>view:nth-child(1) {
  align-items: flex-start;
}

.projectlist {
  display: flex;
  flex-wrap: wrap;
}

.selectInfo>view:nth-child(2) {
  margin-top: 17rpx;
  margin-bottom: 40rpx;
}

.selectInfo image {
  width: 29rpx;
