}

#selectOther input {
  margin-right: 63rpx;
}

picker {
  margin-right: 63rpx;
}

.picker {
  text-align: right;
  min-width: 200rpx;
  min-height: 45rpx;
  line-height: 45rpx;
}

#tip input {
  padding: 22rpx 25rpx;
