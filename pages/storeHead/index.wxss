  display: flex;
  justify-content: between;
}

#projects2 .projectArrow, .peopleList2 .peopleArrow {
  width: 83rpx;
  height: 64rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.projectArrow image, .peopleList2 .peopleArrow image {
  width: 26rpx;
  height: 30rpx;
}

#projects2 scroll-view {
  width: 557rpx;
  height: 96rpx;
  white-space: nowrap;
  margin: 0 auto;
}

.project2 {
  min-width: 10rpx;
  height: 60rpx;
  line-height: 67.5rpx;
  display: inline-block;
  padding: 0 38rpx;
  border-radius: 12rpx;
  margin-right: 18rpx;
  font-size: 26rpx;
}

.selectInfo {
  display: flex;
  flex-direction: column;
  width: 100%;
  font-size: 26rpx;
  color: rgb(153, 153, 153);
}

.selectInfo>view {
  margin-left: 25rpx;
  margin-right: 25rpx;
