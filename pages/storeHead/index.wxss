  width: 100%;
  font-size: 26rpx;
  text-align: center;
  border-radius: 8rpx;
  line-height: 80rpx;
  margin-top: 48rpx;
  margin-bottom: 40rpx;
}

.khxx, .orderForOther_histroy {
