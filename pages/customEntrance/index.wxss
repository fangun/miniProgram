.container {
    padding: 0;
}

/* 页面标题 */
.pageTitle {
    width: 100%;
    background-color: rgba(243,67,67,1);
    
    font-family: PingFangSC-Medium;
    font-size: 36rpx;
    text-align: center;
    color: #fff;

    padding-top: 27rpx;
    padding-bottom: 27rpx;
}

/* 搜索 */
.searchColumn {
    display: flex;
    width: 100%;
    padding-left: 30rpx;
    padding-right: 30rpx;
    background-color: rgba(243,67,67,1);

    box-sizing: border-box;
    padding-bottom: 28rpx;
}

.searchColumn .richScan{
    width: 84rpx; 
}

.searchColumn .store {
    display: flex;
    width:68rpx;
    flex-direction:column;
    align-items:flex-end;
}

.searchColumn .richScan  view,
.searchColumn .store view {
    font-family: PingFangSC-Regular;
    font-size: 18rpx;
    letter-spacing: 2rpx;
    color: #fff;
}

.searchColumn .richScan image,
.searchColumn .store image {
    display: block;
    color: #fff;
    margin-bottom: 7rpx;
}

.searchColumn .richScan image {
    width: 34rpx;
    height: 34rpx;
    margin-left: 10rpx;
}

.searchColumn .store image {
    width: 40rpx;
    height: 34rpx;

    right:0;
}


.searchColumn .search {
    flex: auto;
    background-color: #fff;
    border-radius: 30rpx;

    display: flex;
    height: 65rpx;
    align-items: center;
}

.searchColumn .search image {
    display: block;
    width: 30rpx;
    height: 30rpx;
    padding-left: 30rpx;
    padding-right: 20rpx;
}


.searchColumn .search input {
    font-family: PingFangSC-Regular;
    font-size:28rpx;
    color:rgb(153,153,153);
    height:100%;
    width:90%;
}


/* 热榜 */
.contentTitle {
    position: relative;
    padding-left:119rpx;
    padding-right:119rpx;
    padding-top: 30rpx;
    padding-bottom: 45rpx;
}

.contentTitle image {
    display:block;
    width:100%;
    height:30rpx;
    margin-top: 20rpx;
    margin-bottom: 20rpx;
}

.titleBox {
    position: absolute;
    top: 30rpx;
    right: 0;
    bottom: 0;
    left: 0;
    z-index: 4;

    text-align: center;
}

.titleBox .cTitle {
    font-size: 30rpx;
    color: rgb(51,51,51);
}

.titleBox .eTitle {
    font-size: 18rpx;
    color: rgb(153,153,153);
}

.appointmentHotlist,
.appointmentDiary {
    width:100%;
}

.appointmentHotlist .box {
    padding-left:30rpx;
    padding-right:30rpx;
}

.appointmentHotlist .box .row {
    display: flex;
    flex-wrap:wrap;
    justify-content:space-between
}

.appointmentHotlist .box .row .item {
    position: relative;
    box-sizing: border-box;
    width: 124rpx;
    height: 214rpx;
}

.box .row .item view {
    font-family: PingFangSC-Regular;
    color: rgb(51,51,51);
    font-size: 28rpx;
    text-align: center;
    margin-top: 20rpx;
}

.box .row .item image {
    display: block;
    width: 124rpx;
    height: 124rpx;
    border-radius: 20rpx;
}

.box .row:nth-child(2) .item:nth-child(4){
    padding-left:12rpx;
    padding-top: 2rpx;
}

.box .row:nth-child(2) .item:nth-child(4) image{
    width: 112rpx;
    height: 120rpx;
    border-radius: 0rpx;
}

.box .row .item .icon {
    position: absolute;

    width: 48rpx;
    height: 57rpx;
    top: -24rpx;
    right: -28.5rpx;
    z-index: 3;
}

/* 预约日记 */
.appointmentDiary {
    border-top: 20rpx solid #eee;
}

/* tab */
.tabList {
    display: flex;
    border:2px solid rgb(288,28,28);
    border-radius: 20rpx;
    height: 68rpx;

    margin-left: 30rpx;
    margin-right: 30rpx;
}

.tabList .tab {
    flex: 1;

    font-family: PingFangSC-Regular;
    color: rgb(288,28,28);
    
    font-size: 28rpx;
    line-height: 68rpx;
    text-align: center;
}

.tabList .tab.active {
    background-color: red;
    color: rgb(255,255,255);
}

.tabBox {
    background-color: rgb(248,248,248);
    box-sizing: border-box;
    margin-top:40rpx;

    position: relative;
}

.tabBox .tabContentList {
    position: absolute;
    top: 0;
    bottom: 0;
    z-index: 10;

    width: 200%;
    display: flex;

    transition:left .5s ease .2s;
}

.tabBox .tabContentList .tabContent {
    flex: 1;
    padding-left: 30rpx;
    padding-right: 30rpx;
}

.card {
    background-color: #fff;
    border-radius: 20rpx;
    border:2px solid red;
    margin-bottom: 30rpx;
}

.card .title {
    background-color: rgb(288,28,28);
    color:#fff;
    font-family: PingFangSC-Regular;
    font-size: 28rpx;

    text-align: center;
    padding-top: 15rpx;
    padding-bottom: 15rpx;

}

.card .item {
    position: relative;
    padding: 34rpx 0 28rpx 0;
    margin-left: 30rpx;
    margin-right: 30rpx;
    border-bottom:1px solid #eee;
}

.card .item:last-child{
    border-bottom:none;
}

.card .item .header {
    display: flex;
    line-height: 40rpx;
    align-items:center;
}

.card .item .header .tag {
    padding: 0 20rpx;
    background-color: rgb(243,67,67);
    color:#fff;
    font-family: PingFangSC-Regular;
    font-size: 20rpx;
    border-radius: 6rpx;
    text-align: center;
 
}

.card .item .header .project {
    padding: 0 20rpx;
    color:rgb(51,51,51);
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
}

.card .item .header .time {
    flex: 197rpx;

    color:rgb(243,67,67);
    font-family: PingFangSC-Regular;
    font-size: 28rpx;

    text-align: right;
}

.card .item .clause {
    display: flex;
    font-size: 28rpx;
    font-family: PingFangSC-Regular;
    margin-top: 21rpx;
}

.card .item .footer {
    position: absolute;
    right: 0;
    bottom: 28rpx;

    display: flex;
}

.card .item .footer view {
    font-family: PingFangSC-Regular;
    font-size: 22rpx;

    text-align: center;
    line-height: 50rpx;
    width: 100rpx; 
    border-radius: 6rpx;
    margin-left: 14rpx;
}

.card .item .footer .cancel {
    color:rgb(177,177,177);
    border: 1px solid rgb(177,177,177);
}

.card .item .footer .share {
    background-color: rgb(223,44,44);
    color: #fff;
}

/* 折叠 */
.fold {
    margin-left: -30rpx;
    margin-right: -30rpx;
}

.fold .header {
    display: flex;
    justify-content: space-between;
    line-height: 68rpx;
    padding-left: 30rpx;
    padding-right: 30rpx;

    font-family: PingFangSC-Regular;
    font-size: 26rpx;
    color: rgb(153,153,153);
    background-color: #eee;
}

.fold .box .item {
    padding-top: 34rpx;
    padding-bottom: 34rpx;
    
    padding-left: 30rpx;
    padding-right: 30rpx;

    display: flex;
    align-items: center;
    justify-content: space-between;
}

.fold .box .item .logo image {
    display: block;
    width: 100rpx;
    height: 100rpx;
}

.fold .box .item .details {
    color: rgb(51,51,51);  
}
.fold .box .item .details view:first-child{
    font-family: PingFangSC-Medium;
    font-size: 30rpx;
}

.fold .box .item .details view:nth-child(2){
    font-family: PingFangSC-Regular;
    font-size: 27rpx;
}

.fold .box .item .time {
    font-family: PingFangSC-Regular;
    font-size: 26rpx;  
}

.fold .box .item .time view:first-child {
    color: rgb(153,153,153); 
}

.fold .box .item .time view:nth-child(2) {
    color: rgb(243,67,67); 
}

.fold .box .item .state {
    font-family: PingFangSC-Regular;
    font-size: 27rpx;
    color: rgb(243,67,67);    
}
