page {
    background-color: #eee;
}

.container {
    display: block;
    padding: 88rpx 0 0 0;
}

.pageTitle {
    position: fixed;
    top:0;
    left:0;
    right:0;
    height: 88rpx;
    line-height: 88rpx;

    background-color:rgba(243,67,67,1);
    font-family:PingFangSC-Medium;
    font-size:36rpx;
    text-align:center;
    color:#fff;
}

.card {
    display: flex;
    justify-content:flex-start;
    flex-wrap: wrap;
    padding-left: 10rpx;
    padding-right: 10rpx;
    margin:30rpx;

    border-radius: 10rpx;
    background-color: #fff;

    box-shadow:2px 2px 2px rgba(0,0,0,.2);
}

.card > view {
    margin:30rpx;
    width: 160rpx;
    height:160rpx;

}

.card > view > image {
    display: block;
    width: 100%;
    height: 100%;

    border-radius: 50%;
}

.test {
    height: 500px;
}